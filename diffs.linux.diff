<       break
<     fi
<   done
< 
<   cd ..
<   rm -rf conftest.dir
< else
<   am_cv_$1_dependencies_compiler_type=none
< fi
< ])
< AC_SUBST([$1DEPMODE], [depmode=$am_cv_$1_dependencies_compiler_type])
